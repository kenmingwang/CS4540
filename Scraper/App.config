<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <appSettings>
    <!--University of Utah CS class schedule URL-->
    <add key ="ClassScheduleUrl" value ="https://student.apps.utah.edu/uofu/stu/ClassSchedules/main/" />
    <add key ="EndUrl" value ="/class_list.html?subject=CS"/>

    <!--University of Utah all class catalog-->
    <add key="CourseCatalog" value="https://catalog.utah.edu"/>
    
    <!--Default app settings-->
    <add key ="ConsiderSection" value ="001"/>
    <add key ="CourseRangeStart" value ="1000" />
    <add key ="CourseRangeEnd" value ="7000"/>

    <add key ="Filters" value ="Seminar,Special Topic"/>
    
    <!--Semester notation-->
    <add key ="Spring" value ="4"/>
    <add key ="Summer" value ="6"/>
    <add key ="Fall" value ="8"/>

    <add key ="SeatingAvailibilityLinkText" value="Seating availability for all CS classes"/>

  </appSettings>
</configuration>